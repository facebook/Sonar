    "eslint-plugin-header": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-relay": "^0.0.20",
    "flow-bin": "^0.76.0",
    "glob": "^7.1.2",
    "prettier": "1.13.6"
